{
  "extends": "@y/typescript-config/nextjs.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@y/ui/*": ["../../packages/ui/src/*"],
      "@y/store/*": ["../../packages/store/src/*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "next.config.mjs",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
, "../../packages/ui/src/containers/app-sidebar.tsx"  ],
  "exclude": ["node_modules"]
}
